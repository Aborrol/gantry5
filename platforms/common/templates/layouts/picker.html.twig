<h1>Layout Presets</h1>
<ul>
    {% for name,layout in presets.layouts %}
        <li><a data-g5-lm-picker="{{ layout | json_encode | e('html_attr') }}" data-g5-ajaxify data-g5-ajaxify-target="[data-g5-content]" data-g5-ajaxify-href="{{ gantry.route('pages/create') }}">{{ layout.name|e }}</a></li>
    {% endfor %}
</ul>
