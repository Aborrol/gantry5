<div class="g5-lm-particles-picker">
    <div class="search">
        <input type="text" placeholder="Search..." />
    </div>
    <ul>
        {% for key,list in particles %}
            {% for particle in list %}
            <li class="g5-lm-particle-{{ key | e }}" data-g5-lm-particle="{{ key | e }}">{{ particle | e }}</li>
            {% endfor %}
        {% endfor %}
    </ul>
</div>